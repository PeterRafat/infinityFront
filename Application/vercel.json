{
    "version": 2,
    "builds": [
        {
            "src": "dist/infinity/index.html",
            "use": "@vercel/static",
            "env": {
                "NG_APP_BASEURL2": "https://infinity.runasp.net"
              }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}